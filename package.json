  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/power-assert": "^1.5.8",
    "@types/sinon": "^17.0.3",
    "espower-typescript": "^10.0.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "power-assert": "^1.6.1",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.10",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^4.9.5"
  }
